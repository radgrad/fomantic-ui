/*******************************
         Site Overrides
*******************************/

/* Responsive Breakpoints */
@media only screen and (max-width: 767px) {
    .mobile.hidden,
    .tablet.only,
    .small.monitor.only,
    .large.monitor.only {
        display: none !important;
    }
}
@media only screen and (min-width: 768px) and (max-width: 991px) {
    .mobile.only,
    .tablet.hidden,
    .small.monitor.only,
    .large.monitor.only {
        display: none !important;
    }
}
@media only screen and (min-width: 992px) and (max-width: 1199px) {
    .mobile.only,
    .tablet.only,
    .small.monitor.hidden,
    .large.monitor.only {
        display: none !important;
    }
}
@media only screen and (min-width: 1200px) {
    .mobile.only,
    .tablet.only,
    .small.monitor.only,
    .large.monitor.hidden {
        display: none !important;
    }
}

/* Miscellaneous */
.ui.right.floated {
  float: right;
}

a {
  color: @link-color;
  font-weight: bold;
}

a:hover {
  color: @link-hover-color;
}

.radgrad-header-number {
  color: rgba(0,0,0,.6);
  font-weight: 600;
}

.radgrad-ice-header {
  float: right;
  padding-bottom: 5px;
}

.radgrad-ice-header .ui.label:nth-child(1) {
  padding: 0 !important;
  margin: 0 important;
  background-color: #80AD27 !important;
}

.radgrad-ice-header .ui.label:nth-child(2) {
  padding: 0 important;
  margin: 0 important;
  background-color: #26407C !important;
}

.radgrad-ice-header .ui.label:nth-child(3) {
  padding: 0 important;
  margin: 0 5px 0 0important;
  background-color: #952263 !important;
}

.radgrad-help {
  padding: 14px 14px 0px 14px !important;
}

/*  Background and foreground */
.radgrad-background-color {
  background: @radgrad-BackgroundColor;
}

/* Fonts */
.radgrad-brand-font {
  font-family: 'Nunito';
}

.radgrad-extra-bold-font {
  font-family: 'Montserrat';
  font-weight: 700;
}

/* ICE Font and Background Colors */
.ice-competency-background {
  background: @iceCompetencyColor;
}

.ice-innovation-background {
  background: @iceInnovationColor;
}

.ice-experience-background{
  background: @iceExperienceColor;
}

.ice-competency-color {
  color: @iceCompetencyColor !important;
}

.ice-competency-proj-color {
  color: @iceCompetencyProjColor !important;
}

.ice-innovation-color {
  color: @iceInnovationColor !important;
}

.ice-innovation-proj-color {
  color: @iceInnovationProjColor !important;
}

.ice-experience-color {
  color: @iceExperienceColor !important;
}

.ice-experience-proj-color {
  color: @iceExperienceProjColor !important;
}

/****************************
      GLOBAL ICE CIRCLES
****************************/

/* Circle Style */
.radgrad-ice {
  width: 120px;
  height: 120px;
  margin-left: auto;
  margin-right: auto;
}

.radgrad-ice.menu {
  width: 50px;
  height: 50px;
  margin-left: auto;
  margin-right: auto;
}

.radgrad-ice.menu .radgrad-ice-circle {
  font-size: 50px;
}

.radgrad-ice.degree {
  width: 50px;
  height: 50px;
  margin-left: auto;
  margin-right: auto;
}

.radgrad-ice.degree .radgrad-ice-circle {
  font-size: 50px;
}

.radgrad-ice > .radgrad-ice-circle {
 text-align: center;
 display: block;
 margin-left: auto;
 margin-right: auto;
}

.radgrad-ice-stat {
  position: absolute;
  width: 100%;
  z-index: 1;
  left: 0;
  top: 32px;
  width: 3.45em;
  font-size: 0.28em;
}

.radgrad-ice.menu .radgrad-ice-stat {
  position: absolute;
  z-index: 1;
  left: 0;
  top: 13px;
  width: 50px;
  font-size: .40em;
}

.degreeICE .grid .column .radgrad-ice .radgrad-ice-circle .radgrad-ice-stat {
  top: 15px !important;
}

.radgrad-ice-stat hr {
  height: 10px;
  visibility: hidden;
  margin: 0;
  padding: 0;
}

/* Innovation Font Style */
.radgrad-proj-innov span:nth-of-type(1) {
  color: @iceInnovationColor;
  font-weight: bold;
}

.radgrad-proj-innov span:nth-of-type(2) {
  color: @iceInnovationProjColor;
  margin-top: 30px;
}

.radgrad-ice.degree .radgrad-earn-innov .radgrad-ice-stat > span {
   color: @iceInnovationColor !important;
   font-weight: bold;
}

.radgrad-ice.degree .radgrad-proj-innov .radgrad-ice-stat > span {
  color: @iceInnovationProjColor;
  font-weight: normal;
}

/* Competency Font Style */
.radgrad-proj-comp span:nth-of-type(1) {
  color: @iceCompetencyColor;
  font-weight: bold;
}

.radgrad-proj-comp span:nth-of-type(2) {
  color: @iceCompetencyProjColor;
  margin-top: 30px;
}

.radgrad-ice.degree .radgrad-earn-comp .radgrad-ice-stat > span {
  color: @iceCompetencyColor;
  font-weight: bold;
}

.radgrad-ice.degree .radgrad-proj-comp .radgrad-ice-stat > span {
  color: @iceCompetencyProjColor;
  font-weight: normal;
}

/* Experience Font Style */
.radgrad-proj-exp span:nth-of-type(1) {
  color: @iceExperienceColor;
  font-weight: bold;
}

.radgrad-proj-exp span:nth-of-type(2) {
  color: @iceExperienceProjColor;
  margin-top: 30px;
}

.radgrad-ice.degree .radgrad-earn-exp .radgrad-ice-stat > span {
  color: @iceExperienceColor;
  font-weight: bold;
}

.radgrad-ice.degree .radgrad-proj-exp .radgrad-ice-stat > span {
  color: @iceExperienceProjColor;
  font-weight: normal;
}

