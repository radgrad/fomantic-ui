.radgrad-ice-circle {
  position: relative;
  font-size: 120px; /*controls entire graph size*/
  width: 1em;
  height: 1em;
  border-radius: 50%;
  float: left;
  margin: 0 0.1em 0.1em 0;
  background-color: #EFEFF1; /*Remainder bar graph color*/
}
.radgrad-ice-circle *,
.radgrad-ice-circle *:before,
.radgrad-ice-circle *:after {
  -webkit-box-sizing: content-box;
  -moz-box-sizing: content-box;
  box-sizing: content-box;
}
.radgrad-ice-circle.center {
  float: none;
  margin: 0 auto;
}
.radgrad-ice-circle.big {
  font-size: 240px;
}
.radgrad-ice-circle.small {
  font-size: 80px;
}
.radgrad-ice-circle.big > span {
  position: absolute;
  width: 100%;
  z-index: 1;
  left: -16px;
  right: 0px;
  bottom: 0;
  top: -30px;
  margin-left: auto;
  margin-right: auto;
  width: 5em;
  line-height: 5em;
  font-size: 0.25em; /*Controls span font size*/
  color: #cccccc;
  display: block;
  text-align: center;
  white-space: nowrap;
}

.radgrad-ice-circle:after {
  position: absolute;
  top: 0.08em;
  left: 0.08em;
  display: block;
  content: " ";
  border-radius: 50%;
  background-color: white; /*Center background color*/
  width: 0.84em;
  height: 0.84em;
  /* border: solid 2px #D5D5D7; */
}

.radgrad-ice-menu .radgrad-ice-circle:after {
  position: absolute;
  top: 0em;
  left: 0em;
  display: block;
  content: " ";
  border-radius: 50%;
  background-color: red; /*Center background color*/
  border: solid 7px white;
  width: 1em;
  height: 1em;
  /* border: solid 2px #D5D5D7; */
}

.radgrad-ice-circle .slice {
  position: absolute;
  width: 1em;
  height: 1em;
  clip: rect(0em, 1em, 1em, 0.5em);
}

